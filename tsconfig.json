{
    "exclude": ["node_modules"],
    "include": ["client"],
    "compilerOptions": {
        "paths": { "*": ["web_modules/.types/*"] },
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "jsx": "preserve",
        "baseUrl": "./",
        "allowSyntheticDefaultImports": true,
        "importsNotUsedAsValues": "error"
    }
}
